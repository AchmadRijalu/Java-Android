<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".readUser">

    <TextView
        android:id="@+id/get_nama"
        android:layout_width="288dp"
        android:layout_height="54dp"
        android:layout_marginTop="24dp"
        android:fontFamily="sans-serif-black"
        android:text="Value"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/get_umur"
        android:layout_width="291dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-black"
        android:text="value"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/get_nama"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/get_nama"
        app:layout_constraintTop_toBottomOf="@+id/get_nama" />

    <TextView
        android:id="@+id/get_alamat"
        android:layout_width="293dp"
        android:layout_height="47dp"
        android:layout_marginTop="28dp"
        android:fontFamily="sans-serif-black"
        android:text="value"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/get_umur"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/get_umur"
        app:layout_constraintTop_toBottomOf="@+id/get_umur" />

    <Button
        android:id="@+id/get_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Edit"
        app:layout_constraintEnd_toEndOf="@+id/get_alamat"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="@+id/get_alamat"
        app:layout_constraintTop_toBottomOf="@+id/get_alamat" />

    <Button
        android:id="@+id/get_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Delete"
        app:layout_constraintEnd_toEndOf="@+id/get_edit"
        app:layout_constraintStart_toStartOf="@+id/get_edit"
        app:layout_constraintTop_toBottomOf="@+id/get_edit" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="164dp"
        android:layout_height="177dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/person" />

</androidx.constraintlayout.widget.ConstraintLayout>